  "landing": {
    "hero": {
      "title": "Property Management Made Simple",
      "subtitle": "Manage your properties, collect rent, and communicate with tenants all in one platform. Custom dashboards for each property with your own branding.",
      "cta": "Start Free Trial",
      "demo": "Watch Demo"
    },
    "features": {
      "title": "Everything You Need",
      "subtitle": "Comprehensive property management tools designed for modern landlords and property managers.",
      "feature1": {
        "title": "Multi-Tenant Platform",
        "description": "Manage multiple properties and tenants from a single dashboard with custom branding for each property."
      },
      "feature2": {
        "title": "Automated Rent Collection",
        "description": "Collect rent automatically via Stripe, with payment reminders and late fee calculations."
      },
      "feature3": {
        "title": "Maintenance Tracking",
        "description": "Track maintenance requests, assign to vendors, and keep tenants updated on progress."
      },
      "feature4": {
        "title": "Tenant Portal",
        "description": "Give tenants access to pay rent, submit maintenance requests, and communicate with you."
      },
      "feature5": {
        "title": "Document Management",
        "description": "Store and share leases, notices, and other important documents securely."
      },
      "feature6": {
        "title": "Financial Reporting",
        "description": "Generate income and expense reports, export to CSV or PDF, and track your property performance."
      }
    },
    "pricing": {
