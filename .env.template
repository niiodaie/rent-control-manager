# Rent Control Frontend - Environment Variables Template
# Copy this file to .env.local for local development
# Configure these variables in Vercel dashboard for production

# API Configuration
VITE_API_URL=https://your-backend-api.render.com
VITE_API_VERSION=v1

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# Stripe Configuration
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key
VITE_STRIPE_CONNECT_CLIENT_ID=ca_your-stripe-connect-client-id

# App Configuration
VITE_APP_NAME=Rent Control
VITE_APP_URL=https://rent-control.net
VITE_APP_DOMAIN=rent-control.net

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_CHAT_SUPPORT=true
VITE_ENABLE_NOTIFICATIONS=true

# Google AdSense (for Free Plan)
VITE_GOOGLE_ADSENSE_CLIENT=ca-pub-your-adsense-client-id

# Development
VITE_NODE_ENV=production
VITE_DEBUG_MODE=false

# Internationalization
VITE_DEFAULT_LANGUAGE=en
VITE_ENABLE_GEO_DETECTION=true

# Analytics
VITE_GOOGLE_ANALYTICS_ID=G-YOUR-GA4-ID
VITE_HOTJAR_ID=your-hotjar-id

# Support
VITE_SUPPORT_EMAIL=support@rent-control.net
VITE_CONTACT_EMAIL=contact@rent-control.net

# Social Links
VITE_TWITTER_URL=https://twitter.com/rentcontrol
VITE_LINKEDIN_URL=https://linkedin.com/company/rentcontrol
VITE_GITHUB_URL=https://github.com/your-org/rent-control

# Legal
VITE_PRIVACY_POLICY_URL=https://rent-control.net/privacy
VITE_TERMS_OF_SERVICE_URL=https://rent-control.net/terms
VITE_COOKIE_POLICY_URL=https://rent-control.net/cookies

